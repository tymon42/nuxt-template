{
    "eslint.enable": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.eslint": "always",
        "source.organizeImports": "always"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "prettier.enable": false,
}